{
    "name": "CompteUser",
    "base": "User",
    "idInjection": true,
    "options": {
        "validateUpsert": true
    },
    "properties": {
        "nom": {
            "type": "string",
            "required": true
        },
        "prenom": {
            "type": "string",
            "required": true
        },
        "adresse": {
            "type": "string",
            "required": true
        },
        "ville": {
            "type": "string",
            "required": true
        },
        "description": {
            "type": "string",
            "required": true,
            "default": "Je suis quelqu'un de bien."
        },
        "codePostal": {
            "type": "string",
            "required": true
        },
        "avatarUrl": {
            "type": "string"
        },
        "estCompteActif": {
            "type": "boolean",
            "required": true
        },
        "dateNaissance": {
            "type": "date"
        }
    },
    "validations": [],
    "relations": {},
    "acls": [{
        "accessType": "EXECUTE",
        "principalType": "ROLE",
        "principalId": "$everyone",
        "permission": "ALLOW",
        "property": "SeConnecter"
    }, {
        "accessType": "EXECUTE",
        "principalType": "ROLE",
        "principalId": "$everyone",
        "permission": "ALLOW",
        "property": "getUser"
    }],
    "methods": {}
}